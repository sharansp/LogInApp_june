<core:View xmlns:core="sap.ui.core" xmlns:vbm="sap.ui.vbm" xmlns:commons="sap.ui.commons" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
		controllerName="com.test.Controller.LogIn" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Home Page" floatingFooter="true" showFooter="true">
		<headerContent>
			<Button icon="sap-icon://background" tooltip="Change BackGround Wallpaper" press="onPressChangeBG"/>
			<Button icon="Images/icons/color_picker.ico" tooltip="Change BackGround Color" press="handleResponsivePopoverPress"/>
			<Button icon="sap-icon://background" tooltip="Transition BackGround Color" press="handleTransitionBGColor"/>
		</headerContent>
		
		<content>
			<Panel expandable="true" expanded="false" headerText="Login Panel" width="auto" class="sapUiResponsiveMargin">
				<core:Fragment fragmentName="com.test.fragments.Login_Panel" type="XML" /> 
			</Panel>
			<Image class="dbIcon" src="Images/icons/dash_board.jpg" tooltip="Nav To DashBoard" press="navToDashBoard"/>
		</content>
	</Page>
</core:View>