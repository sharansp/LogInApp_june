<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplay480_12120"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Address"
			labelSpanL="4"
			labelSpanM="12"
			adjustLabelSpan="false"
			emptySpanL="0"
			emptySpanM="0"
			columnsL="2"
			columnsM="2">
			<f:content>
				<core:Title text="Office" />
				<Label text="Name" />
				<Text text="{SupplierName}" />
				<Label text="Street/No." />
				<Text text="{Street} {HouseNumber}" />
				<Label text="ZIP Code/City" />
				<Text text="{ZIPCode} {City}" />
				<Label text="Country" />
				<Text text="{Country}" />
				<core:Title text="Online" />
				<Label text="Web" />
				<Text text="{Url}" />
				<Label text="Twitter" />
				<Text text="{Twitter}" />
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm 
			editable="false"
			layout="ResponsiveGridLayout"
			title="More"
			labelSpanL="4"
			labelSpanM="12"
			adjustLabelSpan="false"
			emptySpanL="0"
			emptySpanM="0"
			columnsL="2"
			columnsM="2"
			singleContainerFullSize="false">
			<f:content>
				<core:Title text="Contact data" />
				<Label text="Email" />
				<Text text="{EMail}" />
				<Label text="Tel." />
				<Text text="{Tel}" />
				<Label text="SMS" />
				<Text text="{Sms}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>